[
    {
        "image": "${image_url}",
        "networkMode": "host",
        "memoryReservation": ${memory},
        "name": "${container_name}",
        "command": [
            "agent",
            "-join",
            "${consul_ip}",
            "-client_addr=0.0.0.0"
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${log_group_name}",
                "awslogs-region": "${log_group_region}"
            }
        }
    }
]



