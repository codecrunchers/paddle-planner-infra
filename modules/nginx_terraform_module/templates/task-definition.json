	[
		{
			"essential": true,
			"image": "${image_url}",
			"memoryReservation": ${memory},
    			"name": "${container_name}",
                        "dnsServers": [
                          "${consul_private_ip}"
                        ],
			"portMappings": [
				{
					"containerPort": ${container_port},
					"hostPort": ${host_port}
				}
			],
			"environment": [],
            "logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "${log_group_name}",
					"awslogs-region": "${log_group_region}"
				}
			}
		}
	]


